{
  "name": "izone-slack",
  "version": "0.0.10",
  "description": "REST API for integrating izone functionality with Slack.",
  "main": "index.js",
  "watch": {
    "test": {
      "patterns": [
        "lib",
        "test"
      ],
      "extensions": "js",
      "quiet": false,
      "legacyWatch": true,
      "inherit": true
    }
  },
  "scripts": {
    "pretest": "node_modules/standard/bin/cmd.js | node_modules/snazzy/bin/cmd.js",
    "start": "npm-watch",
    "test": "mocha ./test --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Iteam1337/izone-slack.git"
  },
  "author": "Alexander Czigler <src@ilix.se>",
  "contributors": [
    {
      "name": "Alexander Czigler",
      "email": "src@ilix.se"
    }
  ],
  "license": "ISC",
  "homepage": "https://iteam.se",
  "dependencies": {
    "google-auth-library": "^0.10.0",
    "googleapis": "^16.1.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "mssql": "^3.3.0",
    "nconf": "^0.8.4",
    "restify": "^4.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "npm-watch": "^0.2.0",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "snazzy": "^6.0.0",
    "standard": "^9.0.1"
  },
  "standard": {
    "globals": [
      "describe",
      "xdescribe",
      "it",
      "xit",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "context"
    ]
  }
}
